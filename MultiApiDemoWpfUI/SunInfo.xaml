<Window x:Class="MultiApiDemoUI.SunInfo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MultiApiDemoUI"
      mc:Ignorable="d" FontSize="22"
      Height="450" Width="800"
      WindowStartupLocation="CenterScreen"
      Title="SunInfo">

    <Grid>
        
        <StackPanel>
            <Button x:Name="loadSunInfo" Margin="20" Click="loadSunInfo_Click">Load Sun Info</Button>
            <Button x:Name="close" Margin="20" Click="close_Click">Close</Button>
            <TextBlock x:Name="suriseText" Margin="20" />
            <TextBlock x:Name="sunsetText" Margin="20" />
        </StackPanel>
    </Grid>
</Window>
